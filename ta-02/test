@Override
    @PostMapping(value = "/cash-management/cash-stock-management/v1/cash-stocks:activate")
    public ResponseEntity<?> openCashStock(@Valid CashStockSessionContextRequest request) {
        try {
            CashStockOperationContextResponse response = openCashStockService.openCashStock(request);
            return ResponseEntity.ok(response);
        } catch (CashException exception) {
            log.error(exception.getErrorMessage().toString());
            xalosService.log(request, exception.getErrorMessage());
            return exceptionMapper.convertKashExceptionToErrorMessage(exception);
        } catch (Exception exception) {
            xalosUtils.logInXalosErrorCashStock(request,exception.getMessage(),Constants.ERROR,Constants.XALOS_OPEN_CASH_REQUEST_FCTTYPE,Constants.XALOS_OPEN_CASH_REQUEST_SERVICE_ID,"",request.getEmployeeRobiId());
            throw exception;
        }
    }
